{
  "name": "url-slug",
  "version": "3.0.0-beta.3",
  "description": "Lightweight slug generator, RFC 3986 compliant",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "test": "mocha --require esm"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "repository": "https://github.com/stldo/url-slug",
  "keywords": [
    "slug",
    "slugify",
    "url",
    "urlify",
    "url-safe",
    "rfc 3986",
    "string",
    "seo"
  ],
  "author": "stldo (https://github.com/stldo)",
  "license": "MIT"
}

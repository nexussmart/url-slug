{
  "name": "url-slug",
  "version": "3.0.0",
  "description": "Lightweight slug generator, RFC 3986 compliant",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config",
    "jest": "node --experimental-vm-modules node_modules/.bin/jest",
    "lint": "standard --env jest",
    "prepare": "npm run build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "test": "npm run lint && npm run jest"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.4",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "*"
  },
  "repository": "https://github.com/stldo/url-slug",
  "keywords": [
    "slug",
    "slugify",
    "url",
    "urlify",
    "url-safe",
    "rfc 3986",
    "string",
    "seo"
  ],
  "author": "stldo (https://github.com/stldo)",
  "license": "MIT"
}
